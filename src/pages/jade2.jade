extends ../layouts/default.jade

block content
  include ../pages/array
  ul
    each item in myArray
      li #{item}
  ul
    each item in myArray
      if item != "second"
        li #{item}
  ul
    each item,i in myArray
      if i != "3"
        li #{item} #{i}
  div.row.medium-up-2.large-up-3.obj-block
    each item in myObj
      div.columns
        div.obj-holders.text-center
          h2 #{item.title}
          p(class="#{item.className}") #{item.message}
  include ../data/mixin
  div.row
    p
      +name
